{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
      "camelcase":"off",
      "eqeqeq":"off",
      "no-undef":"off",
      "no-use-before-define":"off",
      "no-plusplus":"off",
      "func-names":"off",
      "no-shadow":"off",
      "no-unused-expressions":"warn"
  }
}
